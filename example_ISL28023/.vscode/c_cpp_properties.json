{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/*",
                "${workspaceFolder}/Inc/*",
                "${workspaceFolder}/Drivers/CMSIS/Include/*",
                "${workspaceFolder}/Drivers/STM32H7xx_HAL_Driver/Inc/*",
                "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32H7xx/Include/*",
                "${workspaceFolder}/periph_drivers/**"
            ],
            "defines": [
                "USE_HAL_DRIVER",
                "STM32H743xx"
            ],
            "compilerPath": "${env:ELITEBOARDDIR}/gcc/bin/arm-none-eabi-gcc -mcpu=cortex-m7 -mthumb -mfloat-abi=hard",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm"
        },
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/*",
                "${workspaceFolder}/Inc/*",
                "${workspaceFolder}/Drivers/CMSIS/Include/*",
                "${workspaceFolder}/Drivers/STM32H7xx_HAL_Driver/Inc/*",
                "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32H7xx/Include/*",
                "${workspaceFolder}/periph_drivers/**",
                "${workspaceFolder}/Middlewares/Third_Party/FatFs/src/*",
                "${workspaceFolder}/Middlewares/Third_Party/LwIP/src/include/*",
                "${workspaceFolder}/Middlewares/Third_Party/LwIP/system/*",
                "${workspaceFolder}/Middlewares/Third_Party/LwIP/system/arch/*"
            ],
            "defines": [
                "USE_HAL_DRIVER",
                "STM32H743xx"
            ],
            "compilerPath": "${env:ELITEBOARDDIR}/gcc/bin/arm-none-eabi-gcc -mcpu=cortex-m7 -mthumb -mfloat-abi=hard",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm"
        }
    ],
    "version": 4
}